<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:Motoplay.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:gif="clr-namespace:Avalonia.Labs.Gif;assembly=Avalonia.Labs.Gif"
        xmlns:views="clr-namespace:Motoplay.Views"
		xmlns:local="clr-namespace:Motoplay"
        mc:Ignorable="d" x:Class="Motoplay.Views.MainWindow"
		Icon="/Assets/motoplay-logo.ico"
		Title="Motoplay"
		HorizontalAlignment="Stretch"
		VerticalAlignment="Stretch"
		SystemDecorations="None"
		CanResize="True"
		SizeToContent="Manual"
		Topmost="False"
	    WindowStartupLocation="CenterScreen"
		WindowState="Maximized"
		IsEnabled="True"
		Background="Transparent"
		d:DesignWidth="800" d:DesignHeight="480">
	
	<!-- Window Resources -->
	<Window.Resources>
		<!-- Toast Notification Entry Animation -->
		<Animation x:Key="toastNotificationEntry" x:SetterTargetType="Grid" Duration="0:0:0.5" Delay="0:0:0.0" FillMode="Forward" Easing="QuadraticEaseInOut">
			<KeyFrame Cue="0%">
				<Setter Property="Margin" Value="0, -256, 0, 0"/>
			</KeyFrame>
			<KeyFrame Cue="100%">
				<Setter Property="Margin" Value="0, 0, 0, 0"/>
			</KeyFrame>
		</Animation>
		<!-- Toast Notification Exit Animation -->
		<Animation x:Key="toastNotificationExit" x:SetterTargetType="Grid" Duration="0:0:0.5" Delay="0:0:0.0" FillMode="Forward" Easing="CubicEaseIn">
			<KeyFrame Cue="0%">
				<Setter Property="Margin" Value="0, 0, 0, 0"/>
			</KeyFrame>
			<KeyFrame Cue="100%">
				<Setter Property="Margin" Value="0, -256, 0, 0"/>
			</KeyFrame>
		</Animation>
		<!-- Panel Fade In Cover Animation -->
		<Animation x:Key="panelFadeInCover" x:SetterTargetType="Grid" Duration="0:0:1.0" Delay="0:0:0.0" FillMode="Forward" Easing="LinearEasing">
			<KeyFrame Cue="0%">
				<Setter Property="Opacity" Value="1.0"/>
			</KeyFrame>
			<KeyFrame Cue="100%">
				<Setter Property="Opacity" Value="0.0"/>
			</KeyFrame>
		</Animation>
	</Window.Resources>
	
	
	
	<!-- Root of Window -->
	<Grid Name="windowRoot" Background="White" RowDefinitions="2,*,2" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
		<!-- Top Divisor -->
		<Grid Grid.Row="0" Grid.Column="0" Background="LightGray" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></Grid>
		<!-- Mid Area Container -->
		<Grid Grid.Row="1" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
			<!-- Mid Area Background -->
			<Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
				<!-- Pages Background Area -->
				<Grid Background="White" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
					<!-- Vehicle Panel Background -->
					<Grid Name="pageBgForPanel" IsVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
						<!-- Background for Panel Setup and Connection Screen -->
						<Grid Name="backgroundForPanelSetupAndConnect" IsVisible="False" Background="White" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></Grid>
						<!-- Background for Ready Panel Screen -->
						<Grid Name="backgroundForPanelContent" IsVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
							<Grid ColumnDefinitions="12.6*,2,88*" x:CompileBindings="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
								<Grid Grid.Row="0" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></Grid>
								<Grid Grid.Row="0" Grid.Column="1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></Grid>
								<Grid Grid.Row="0" Grid.Column="2" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
									<!-- Background of Wall Shadow -->
									<Grid RowDefinitions="*,*" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
										<Grid Grid.Row="0" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
											<Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
												<Image Source="/Assets/panel-wall-top.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Stretch="Fill" RenderOptions.BitmapInterpolationMode="HighQuality"></Image>
											</Grid>
										</Grid>
										<Grid Grid.Row="1" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
											<Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0, -70, 0, 0">
												<Image Source="/Assets/panel-wall-bottom.png" HorizontalAlignment="Center" VerticalAlignment="Top" Stretch="Fill" RenderOptions.BitmapInterpolationMode="HighQuality" Margin="0, -22, 0, 0" Width="716" Height="332"></Image>
											</Grid>
										</Grid>
									</Grid>
									<!-- Dark Mode Overlay -->
									<Grid Name="vehiclePanel_background_wallDarkOverlay" IsVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="#000000" Opacity="0.7" Margin="0, -8, 0, 0"></Grid>
									<!-- Background Arcs -->
									<Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
										<Arc Name="vehiclePanel_background_speedArc40Kmh" IsVisible="True" StartAngle="0" SweepAngle="360" StrokeThickness="100" StrokeLineCap="Flat" Stroke="#53707a" HorizontalAlignment="Center" VerticalAlignment="Center" Width="834" Height="834" Margin="0, 0, 0, 24" Opacity="0.7">
											<!-- Effect Disable Because of GPU Performance Problem -->
											<!-- <Arc.Styles>
												<Style Selector="Arc">
													<Setter Property="Effect">
														<BlurEffect Radius="8"></BlurEffect>
													</Setter>
												</Style>
											</Arc.Styles> -->
										</Arc>
										<Arc Name="vehiclePanel_background_speedArc70Kmh" IsVisible="True" StartAngle="0" SweepAngle="360" StrokeThickness="48" StrokeLineCap="Flat" Stroke="#509eba" HorizontalAlignment="Center" VerticalAlignment="Center" Width="608" Height="608" Margin="0, 0, 0, 24" Opacity="0.7">
											<!-- Effect Disable Because of GPU Performance Problem -->
											<!-- <Arc.Styles>
												<Style Selector="Arc">
													<Setter Property="Effect">
														<BlurEffect Radius="8"></BlurEffect>
													</Setter>
												</Style>
											</Arc.Styles> -->
										</Arc>
										<Arc Name="vehiclePanel_background_speedArc100Kmh" IsVisible="True" StartAngle="0" SweepAngle="360" StrokeThickness="32" StrokeLineCap="Flat" Stroke="#FA972D" HorizontalAlignment="Center" VerticalAlignment="Center" Width="480" Height="480" Margin="0, 0, 0, 24" Opacity="0.7">
											<!-- Effect Disable Because of GPU Performance Problem -->
											<!-- <Arc.Styles>
												<Style Selector="Arc">
													<Setter Property="Effect">
														<DropShadowEffect Color="#fc1e00" BlurRadius="16" OffsetX="0" OffsetY="0" Opacity="1.0"/>
													</Setter>
												</Style>
											</Arc.Styles> -->
										</Arc>
									</Grid>
									<!-- Background of Panel Ground -->
									<Grid RowDefinitions="*,*" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
										<Grid Grid.Row="0" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></Grid>
										<Grid Grid.Row="1" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
											<Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0, -72, 0, 0">
												<Image Name="vehiclePanel_background_groundImage" Source="/Assets/panel-ground.png" HorizontalAlignment="Center" VerticalAlignment="Top" Stretch="Fill" RenderOptions.BitmapInterpolationMode="HighQuality" Margin="0, -22, 0, 0" Width="716" Height="332"></Image>
											</Grid>
										</Grid>
									</Grid>
									<!-- Background Arcs Reflection -->
									<Grid RowDefinitions="*,*" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
										<Grid Grid.Row="0" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></Grid>
										<Grid Grid.Row="1" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
											<Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0, -72, 0, 0">
												<Grid Name="vehiclePanel_background_speedArc100Kmh_reflection" IsVisible="True" HorizontalAlignment="Center" VerticalAlignment="Top" Width="432" Height="432" Margin="0, -57, 0, 24" Opacity="0.15">
													<Grid.RenderTransform>
														<Rotate3DTransform Depth="900">
															<Rotate3DTransform.AngleX>78</Rotate3DTransform.AngleX>
															<Rotate3DTransform.AngleY>0</Rotate3DTransform.AngleY>
															<Rotate3DTransform.AngleZ>0</Rotate3DTransform.AngleZ>
														</Rotate3DTransform>
													</Grid.RenderTransform>
													<Arc Name="vehiclePanel_background_speedArc100Kmh_reflection_arc" StartAngle="0" SweepAngle="112" StrokeThickness="38" StrokeLineCap="Flat" Stroke="#FA972D" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
														<Arc.RenderTransform>
															<TransformGroup>
																<ScaleTransform ScaleX="1.0" ScaleY="1.0"></ScaleTransform>
																<TranslateTransform X="0" Y="0"></TranslateTransform>
																<RotateTransform CenterX="0" CenterY="0" Angle="34"></RotateTransform>
															</TransformGroup>
														</Arc.RenderTransform>
													</Arc>
												</Grid>
												<Grid Name="vehiclePanel_background_speedArc70Kmh_reflection" IsVisible="True" HorizontalAlignment="Center" VerticalAlignment="Top" Width="540" Height="540" Margin="0, -121, 0, 24" Opacity="0.15">
													<Grid.RenderTransform>
														<Rotate3DTransform Depth="800">
															<Rotate3DTransform.AngleX>76</Rotate3DTransform.AngleX>
															<Rotate3DTransform.AngleY>0</Rotate3DTransform.AngleY>
															<Rotate3DTransform.AngleZ>0</Rotate3DTransform.AngleZ>
														</Rotate3DTransform>
													</Grid.RenderTransform>
													<Arc Name="vehiclePanel_background_speedArc70Kmh_reflection_arc" StartAngle="0" SweepAngle="112" StrokeThickness="40" StrokeLineCap="Flat" Stroke="#509eba" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
														<Arc.RenderTransform>
															<TransformGroup>
																<ScaleTransform ScaleX="1.0" ScaleY="1.0"></ScaleTransform>
																<TranslateTransform X="0" Y="0"></TranslateTransform>
																<RotateTransform CenterX="0" CenterY="0" Angle="34"></RotateTransform>
															</TransformGroup>
														</Arc.RenderTransform>
													</Arc>
												</Grid>
												<Grid Name="vehiclePanel_background_speedArc40Kmh_reflection" IsVisible="True" HorizontalAlignment="Center" VerticalAlignment="Top" Width="675" Height="675" Margin="0, -192, 0, 24" Opacity="0.15">
													<Grid.RenderTransform>
														<Rotate3DTransform Depth="700">
															<Rotate3DTransform.AngleX>75</Rotate3DTransform.AngleX>
															<Rotate3DTransform.AngleY>0</Rotate3DTransform.AngleY>
															<Rotate3DTransform.AngleZ>0</Rotate3DTransform.AngleZ>
														</Rotate3DTransform>
													</Grid.RenderTransform>
													<Arc Name="vehiclePanel_background_speedArc40Kmh_reflection_arc" StartAngle="0" SweepAngle="112" StrokeThickness="64" StrokeLineCap="Flat" Stroke="#53707a" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
														<Arc.RenderTransform>
															<TransformGroup>
																<ScaleTransform ScaleX="1.0" ScaleY="1.0"></ScaleTransform>
																<TranslateTransform X="0" Y="0"></TranslateTransform>
																<RotateTransform CenterX="0" CenterY="0" Angle="34"></RotateTransform>
															</TransformGroup>
														</Arc.RenderTransform>
													</Arc>
												</Grid>
											</Grid>
										</Grid>
									</Grid>
								</Grid>
							</Grid>
						</Grid>
						<!-- End of Background for -->
					</Grid>
					<!-- General Metrics Background -->
					<Grid Name="pageBgForMetrics" IsVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
						<Image Source="/Assets/background-charts.png" HorizontalAlignment="Right" VerticalAlignment="Center" Width="432" RenderOptions.BitmapInterpolationMode="HighQuality" Margin="0, 0, -16, -272" Opacity="0.05"></Image>
					</Grid>
					<!-- Music Player Background -->
					<Grid Name="pageBgForPlayer" IsVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
						<Grid Background="Pink"></Grid>
					</Grid>
					<!-- Web Browser Background -->
					<Grid Name="pageBgForBrowser" IsVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
						<Grid Background="Orange"></Grid>
					</Grid>
					<!-- USB Camera Background -->
					<Grid Name="pageBgForCamera" IsVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
						<Grid Background="Red"></Grid>
					</Grid>
					<!-- Mirror Phone Background -->
					<Grid Name="pageBgForPhone" IsVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
						<Grid Background="Green"></Grid>
					</Grid>
					<!-- App Preferences Background -->
					<Grid Name="pageBgForPreferences" IsVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
						<Image Source="/Assets/background-gears.png" HorizontalAlignment="Right" VerticalAlignment="Bottom" Height="300" Opacity="0.03" Margin="0, 0, -28, 0"></Image>
					</Grid>
				</Grid>
				<!-- Menu Bar Background Area -->
				<Grid ColumnDefinitions="15.5*,85.0*" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
					<Grid Grid.Row="0" Grid.Column="0">
						<Image Source="/Assets/menubar-background.png" Stretch="Fill" RenderOptions.BitmapInterpolationMode="HighQuality" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0, -8, 0, -8"></Image>
					</Grid>
					<Grid Grid.Row="0" Grid.Column="1"></Grid>
				</Grid>
			</Grid>
			<!-- Mid Area -->
			<Grid ColumnDefinitions="12.6*,2,88*" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
				<!-- Menu Bar -->
				<Grid Grid.Row="0" Grid.Column="0">
					<Grid RowDefinitions="72,*,72" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="8, 8, 8, 8">
						<Grid Grid.Row="0" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
							<Button Name="rollMenuUp" HorizontalAlignment="Center" VerticalAlignment="Stretch" Width="72" CornerRadius="16" Background="#ededed" Padding="16, 16, 16, 16">
								<Image Source="/Assets/arrow-up.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Opacity="0.7"></Image>
							</Button>
						</Grid>
						<Grid Grid.Row="1" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
							<ScrollViewer Name="menuItensScroll" HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Hidden" Offset="0, 0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0, 8, 0, 8">
								<StackPanel Orientation="Vertical" Spacing="8" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="0, 8, 0, 8">
									<!-- Vehicle Panel Button -->
									<Button Name="menuPanelButton" HorizontalAlignment="Center" VerticalAlignment="Top" Width="72" Height="72" CornerRadius="16" Background="#ededed" Padding="16, 16, 16, 16" BorderThickness="0" BorderBrush="White">
										<Image Source="/Assets/menu-panel-icon.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Opacity="0.7"></Image>
									</Button>
									<!-- General Metrics Button -->
									<Button Name="menuMetricsButton" HorizontalAlignment="Center" VerticalAlignment="Top" Width="72" Height="72" CornerRadius="16" Background="#ededed" Padding="16, 16, 16, 16" BorderThickness="0" BorderBrush="White">
										<Image Source="/Assets/menu-metrics-icon.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Opacity="0.7"></Image>
									</Button>
									<!-- Music Player Button -->
									<Button Name="menuPlayerButton" HorizontalAlignment="Center" VerticalAlignment="Top" Width="72" Height="72" CornerRadius="16" Background="#ededed" Padding="16, 16, 16, 16" BorderThickness="0" BorderBrush="White">
										<Image Source="/Assets/menu-musics-icon.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Opacity="0.7"></Image>
									</Button>
									<!-- Web Browser Button -->
									<Button Name="menuBrowserButton" HorizontalAlignment="Center" VerticalAlignment="Top" Width="72" Height="72" CornerRadius="16" Background="#ededed" Padding="16, 16, 16, 16" BorderThickness="0" BorderBrush="White">
										<Image Source="/Assets/menu-browser-icon.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Opacity="0.7"></Image>
									</Button>
									<!-- USB Camera Button -->
									<Button Name="menuCameraButton" HorizontalAlignment="Center" VerticalAlignment="Top" Width="72" Height="72" CornerRadius="16" Background="#ededed" Padding="16, 16, 16, 16" BorderThickness="0" BorderBrush="White">
										<Image Source="/Assets/menu-camera-icon.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Opacity="0.7"></Image>
									</Button>
									<!-- Mirror Phone Button -->
									<Button Name="menuPhoneButton" HorizontalAlignment="Center" VerticalAlignment="Top" Width="72" Height="72" CornerRadius="16" Background="#ededed" Padding="16, 16, 16, 16" BorderThickness="0" BorderBrush="White">
										<Image Source="/Assets/menu-phone-icon.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Opacity="0.7"></Image>
									</Button>
									<!-- App Preferences Button -->
									<Button Name="menuPreferencesButton" HorizontalAlignment="Center" VerticalAlignment="Top" Width="72" Height="72" CornerRadius="16" Background="#ededed" Padding="16, 16, 16, 16" BorderThickness="0" BorderBrush="White">
										<Image Source="/Assets/menu-preferences-icon.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Opacity="0.7"></Image>
									</Button>
									<!-- App Quit Button -->
									<Button Name="menuQuitButton" HorizontalAlignment="Center" VerticalAlignment="Top" Width="72" Height="72" CornerRadius="16" Background="#ededed" Padding="16, 16, 16, 16">
										<Image Source="/Assets/menu-quit-icon.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Opacity="0.7"></Image>
									</Button>
								</StackPanel>
							</ScrollViewer>
							<!-- Gradients of Border of Menu Itens -->
							<Grid HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="10" Width="10">
								<Grid.RenderTransform>
									<TransformGroup>
										<ScaleTransform ScaleX="1.0" ScaleY="8.4"></ScaleTransform>
										<TranslateTransform X="8" Y="0"></TranslateTransform>
										<RotateTransform CenterX="0" CenterY="0" Angle="90"></RotateTransform>
									</TransformGroup>
								</Grid.RenderTransform>
								<Image Source="/Assets/menubar-background-transition.png" Stretch="Fill" RenderOptions.BitmapInterpolationMode="HighQuality"></Image>
							</Grid>
							<Grid HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Height="10" Width="10">
								<Grid.RenderTransform>
									<TransformGroup>
										<ScaleTransform ScaleX="1.0" ScaleY="8.4"></ScaleTransform>
										<TranslateTransform X="8"  Y="0"></TranslateTransform>
										<RotateTransform CenterX="0" CenterY="0" Angle="-90"></RotateTransform>
									</TransformGroup>
								</Grid.RenderTransform>
								<Image Source="/Assets/menubar-background-transition.png" Stretch="Fill" RenderOptions.BitmapInterpolationMode="HighQuality"></Image>
							</Grid>
						</Grid>
						<Grid Grid.Row="2" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
							<Button Name="rollMenuDown" HorizontalAlignment="Center" VerticalAlignment="Stretch" Width="72" CornerRadius="16" Background="#ededed" Padding="16, 16, 16, 16">
								<Image Source="/Assets/arrow-down.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Opacity="0.7"></Image>
							</Button>
						</Grid>
					</Grid>
				</Grid>
				<!-- Mid Area Divisor -->
				<Grid Grid.Row="0" Grid.Column="1" Background="#a3a3a3"></Grid>
				<!-- Page Area -->
				<Grid Grid.Row="0" Grid.Column="2">
					<!-- Temporary Initialization Logo -->
					<Grid Name="tempInitializationLogo" IsVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
						<Image Source="/Assets/background-motoplay-logo.png" HorizontalAlignment="Center" VerticalAlignment="Center" Width="256" Height="256" Opacity="0.05"></Image>
					</Grid>
					<!-- Vehicle Panel Page Content -->
					<Grid Name="pageContentForPanel" IsVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
						<!-- OBD Adapter Setup -->
						<Grid Name="vehiclePanel_obdSetupScreen" IsVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
							<Grid Name="vehiclePanel_obdSetup_InitialStep" IsVisible="True" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
								<StackPanel Orientation="Vertical" Spacing="16" HorizontalAlignment="Center" VerticalAlignment="Center" MaxWidth="550">
									<Image Source="/Assets/obd-setup-image.png" HorizontalAlignment="Center" VerticalAlignment="Top" Width="128" Opacity="0.75"></Image>
									<TextBlock Text="{DynamicResource ResourceKey=vehiclePanel_setupIntroText}" TextWrapping="Wrap" HorizontalAlignment="Center" VerticalAlignment="Top" TextAlignment="Center"></TextBlock>
									<Button Name="vehiclePanel_obdSetup_startButton" Content="{DynamicResource ResourceKey=vehiclePanel_setupIntroButton}" HorizontalAlignment="Center" VerticalAlignment="Top" Width="128" HorizontalContentAlignment="Center" Margin="0, 16, 0, 0"></Button>
								</StackPanel>
							</Grid>
							<Grid Name="vehiclePanel_obdSetup_Search" IsVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
								<TextBlock Text="{DynamicResource ResourceKey=vehiclePanel_setupSearchTitle}" HorizontalAlignment="Left" VerticalAlignment="Top" FontWeight="Medium" FontSize="16" LineHeight="32" Margin="16, 8, 0, 0"></TextBlock>
								<Rectangle Fill="#ababab" HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="1" Margin="0, 48, 0, 0" Opacity="0.5"></Rectangle>
								<ScrollViewer HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Visible" Offset="0, 0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0, 48, 0, 48">
									<StackPanel Name="vehiclePanel_obdSetup_devicesList" Orientation="Vertical" Spacing="8" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="128, 8, 128, 8">
										<TextBlock Name="vehiclePanel_obdSetup_noDevice" Text="{DynamicResource ResourceKey=vehiclePanel_setupSearchNotFound}" HorizontalAlignment="Stretch" VerticalAlignment="Top" TextAlignment="Center" Opacity="0.5" Margin="0, 16, 0, 0"></TextBlock>
									</StackPanel>
								</ScrollViewer>
								<Rectangle Fill="#ababab" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Height="1" Margin="0, 0, 0, 48" Opacity="0.5"></Rectangle>
								<Button Name="vehiclePanel_obdSetup_refreshScanButton" IsEnabled="False" Content="{DynamicResource ResourceKey=vehiclePanel_setupRefreshScanButton}" HorizontalAlignment="Center" VerticalAlignment="Bottom" Height="32" Width="150" HorizontalContentAlignment="Center" Margin="0, 0, 0, 8"></Button>
							</Grid>
							<Grid Name="vehiclePanel_obdSetup_Password" IsVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
								<TextBlock Text="{DynamicResource ResourceKey=vehiclePanel_setupPasswordTitle}" HorizontalAlignment="Left" VerticalAlignment="Top" FontWeight="Medium" FontSize="16" LineHeight="32" Margin="16, 8, 0, 0"></TextBlock>
								<Rectangle Fill="#ababab" HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="1" Margin="0, 48, 0, 0" Opacity="0.5"></Rectangle>
								<ScrollViewer HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Visible" Offset="0, 0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0, 48, 0, 48">
									<StackPanel Orientation="Vertical" Spacing="8" HorizontalAlignment="Center" VerticalAlignment="Top" MaxWidth="256" Margin="0, 8, 0, 0">
										<Image Source="/Assets/bluetooth-hd-icon.png" HorizontalAlignment="Center" VerticalAlignment="Top" Width="100" Opacity="0.75"></Image>
										<StackPanel Orientation="Vertical" Spacing="4" HorizontalAlignment="Stretch" VerticalAlignment="Top" Width="256">
											<TextBlock Text="{DynamicResource ResourceKey=vehiclePanel_setupPasswordName}" FontSize="12" Opacity="0.5"></TextBlock>
											<TextBox Name="vehiclePanel_obdSetup_inputName" Text="Place Holder" IsEnabled="False"></TextBox>
										</StackPanel>
										<StackPanel Orientation="Vertical" Spacing="4" HorizontalAlignment="Stretch" VerticalAlignment="Top" Width="256">
											<TextBlock Text="{DynamicResource ResourceKey=vehiclePanel_setupPasswordMac}" FontSize="12" Opacity="0.5"></TextBlock>
											<TextBox Name="vehiclePanel_obdSetup_inputMac" Text="Place Holder" IsEnabled="False"></TextBox>
										</StackPanel>
										<StackPanel Orientation="Vertical" Spacing="4" HorizontalAlignment="Stretch" VerticalAlignment="Top" Width="256">
											<TextBlock Text="{DynamicResource ResourceKey=vehiclePanel_setupPasswordPassword}" FontSize="12" Opacity="0.5"></TextBlock>
											<TextBox Name="vehiclePanel_obdSetup_inputPin" Watermark="{DynamicResource ResourceKey=vehiclePanel_setupPasswordHint}" AcceptsReturn="False" AcceptsTab="False"></TextBox>
											<TextBlock Text="{DynamicResource ResourceKey=vehiclePanel_setupPasswordHint2}" FontSize="10" Opacity="0.5"></TextBlock>
										</StackPanel>
									</StackPanel>
								</ScrollViewer>
								<Rectangle Fill="#ababab" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Height="1" Margin="0, 0, 0, 48" Opacity="0.5"></Rectangle>
								<StackPanel Orientation="Horizontal" Spacing="16" HorizontalAlignment="Center" VerticalAlignment="Bottom" Height="32" Margin="0, 0, 0, 8">
