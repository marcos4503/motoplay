			</Grid>

			<!-- Motoplay Status Bar -->
			<Grid HorizontalAlignment="Right" VerticalAlignment="Top" Height="32" Width="2" Margin="0, 8, 8, 0">
				<StackPanel Orientation="Horizontal" Spacing="4" FlowDirection="RightToLeft">
					<!-- Keyboard Toggle Button -->
					<Button Name="toggleKeyboardButton" HorizontalAlignment="Right" VerticalAlignment="Top" Width="32" Height="32" Padding="6, 6, 6, 6" ToolTip.Tip="{DynamicResource ResourceKey=statusBar_toggleKeyboardTooltip}">
						<Image Name="toggleKeyboardButtonImg" Source="/Assets/keyboard-on.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></Image>
					</Button>
					<!-- Keyboard Notification -->
					<Grid Name="toggleKeyboardTip" IsVisible="False" ColumnDefinitions="*,16" FlowDirection="LeftToRight" HorizontalAlignment="Right" VerticalAlignment="Top" Width="256" Height="200" ClipToBounds="True">
						<!-- Style -->
						<Grid.Transitions>
							<Transitions>
								<DoubleTransition Property="Width" Duration="0:0:0.35"/>
								<DoubleTransition Property="Height" Duration="0:0:0.35"/>
							</Transitions>
						</Grid.Transitions>
						<!-- Content -->
						<Grid Grid.Row="0" Grid.Column="1" Width="16" Height="32" HorizontalAlignment="Right" VerticalAlignment="Top">
							<Polygon HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Points="0,16 16,16 0,32" Fill="#00aaff"></Polygon>
						</Grid>
						<Grid Grid.Row="0" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
							<Border HorizontalAlignment="Stretch" VerticalAlignment="Top" Background="#00aaff" CornerRadius="8, 8, 8, 8" Padding="8, 8, 8, 8">
								<TextBlock Text="{DynamicResource ResourceKey=statusBar_toggleKeyboardTipText}" TextWrapping="Wrap" Foreground="White" HorizontalAlignment="Right" VerticalAlignment="Top"></TextBlock>
							</Border>
						</Grid>
					</Grid>
					<!-- Update Notification -->
					<Button Name="updateAppButton" HorizontalAlignment="Right" VerticalAlignment="Top" Width="32" Height="32" Padding="6, 6, 6, 6" ToolTip.Tip="{DynamicResource ResourceKey=statusBar_updateTooltip}">
						<Image Source="/Assets/update-icon.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></Image>
					</Button>
					<!-- Binded CLI Notification -->
					<Button Name="bindedCliButton" HorizontalAlignment="Right" VerticalAlignment="Top" Width="32" Height="32" Padding="6, 6, 6, 6" ToolTip.Tip="{DynamicResource ResourceKey=statusBar_bindedCliTooltip}">
						<Image Source="/Assets/binded-cli-icon.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></Image>
					</Button>
					<!-- Binded BluetoothCTL Notification -->
					<Button Name="bindedbluetoothCtlButton" HorizontalAlignment="Right" VerticalAlignment="Top" Width="32" Height="32" Padding="6, 6, 6, 6" ToolTip.Tip="{DynamicResource ResourceKey=statusBar_bindedBluetoothCtlTooltip}">
						<Image Source="/Assets/binded-bluetoothctl-icon.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></Image>
					</Button>
					<!-- Trying to Connect to Paired OBD Adapter Notification -->
					<Button Name="tryingConnectToObdButton" HorizontalAlignment="Right" VerticalAlignment="Top" Width="32" Height="32" Padding="6, 6, 6, 6" ToolTip.Tip="{DynamicResource ResourceKey=statusBar_tryingToConnecToObdTooltip}">
						<Image Source="/Assets/connecting-to-obd-icon.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></Image>
					</Button>
					<!-- Connected To OBD Adapter Notification -->
					<Button Name="connectedToObdButton" HorizontalAlignment="Right" VerticalAlignment="Top" Width="32" Height="32" Padding="6, 6, 6, 6" ToolTip.Tip="{DynamicResource ResourceKey=statusBar_connectedToObdTooltip}">
						<Image Source="/Assets/connected-to-obd-icon.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></Image>
					</Button>
					<!-- Tasks Display -->
					<gif:GifImage Name="runningTasksIndicator" HorizontalAlignment="Right" VerticalAlignment="Top" IsVisible="True" Width="32" Height="32" Source="avares://Motoplay/Assets/running-tasks-2.gif" Stretch="Fill" />
				</StackPanel>
			</Grid>

			<!-- Toast Notification -->
			<Grid Name="toastNotificationRoot" IsVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="0, 0, 0, 0">
				<Border Name="toastNotificationBg" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="128, 8, 128, 0" CornerRadius="8" Background="#429ebd" BorderBrush="#32778f" BorderThickness="2" BoxShadow="0 4 5 0 #d1d1d1">
					<Grid RowDefinitions="*,2" HorizontalAlignment="Stretch" VerticalAlignment="Top">
						<Grid Grid.Row="0" Grid.Column="0" ColumnDefinitions="*,8,80" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="8,8,8,8">
							<Grid Grid.Row="0" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Center">
								<TextBlock Name="toastNotificationText" Text="Place Holder." TextWrapping="Wrap" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="White"></TextBlock>
							</Grid>
							<Grid Grid.Row="0" Grid.Column="1"></Grid>
							<Grid Grid.Row="0" Grid.Column="2" HorizontalAlignment="Stretch" VerticalAlignment="Center">
								<Button Name="toastNotificationDismissButton" Content="{DynamicResource ResourceKey=toastNotification_dismissButton}" HorizontalAlignment="Stretch" VerticalAlignment="Top" HorizontalContentAlignment="Center" Foreground="White"></Button>
							</Grid>
						</Grid>
						<Grid Grid.Row="1" Grid.Column="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="3, 0, 3, 0">
							<Border HorizontalAlignment="Stretch" VerticalAlignment="Stretch" CornerRadius="0, 0, 8, 8" ClipToBounds="True">
								<ProgressBar Name="toastNotificationTimeBar" Maximum="100" Value="50" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Foreground="#00cefc" MinHeight="0" CornerRadius="0"></ProgressBar>
							</Border>
						</Grid>
					</Grid>
				</Border>
			</Grid>

			<!-- Interaction Blocker -->
			<Grid Name="appInteractionBlocker" IsVisible="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="#000000" Opacity="0.75"></Grid>
			
		</Grid>
		<!-- Bottom Divisor -->
		<Grid Grid.Row="2" Grid.Column="0" Background="LightGray" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></Grid>
	</Grid>
	
</Window>
